int reverse(int x){
    if(x==-2147483648){
        return 0;
    }
    int temp=x,res;
    long sum=0;
    if(x<0){
        temp=-x;
    }
    while(temp>0){
        sum=(sum*10)+(temp%10);
        temp=temp/10;
    }
    if(sum>INT_MAX ){
        return 0;
    }
    else{
        res=(int)sum;
    }
    if(x<0){
        return -res;
    }
    return res;
}
