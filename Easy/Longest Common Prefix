#include <stdio.h>
#include <stdlib.h>
char* longestCommonPrefix(char** strs, int strsSize) {
    char* common=(char*)malloc(strsSize*sizeof(char));
    
    return common;
}
int main() {
    char* strs[] = {"flower","flow","fam","flight"};
    int strsSize=4;
    printf("%s\n", strs[1]);
    // for(int i =0;i<200;i++){
    printf("%c\n",strs[1][0]);
    // }
    int i =0;
    while(strs[0][i]==strs[1][i]){
        i++;
    }
    printf("%d\n",i);
    char res1[i];
    for(int j=0;j<i;j++){
        res1[j]=strs[0][j];
    }
    res1[i]='\0';
    printf("%s\n",res1);
    int l;
    for(int k=0;k<strsSize;k++){
        l=0;
        while(res1[l]==strs[k][l]){
            l++;
        }
        for(int m=0;m<l;m++){
            res1[m]=strs[0][m];
        }
        res1[l]='\0';
        printf("%s\n",res1);
    }
    printf("%d",l);
    // char* resfinal=(char*) malloc(l*sizeof(char));
    // for(int m=0;m<l;m++){
    //         resfinal[m]=strs[0][m];
    // } 
    // if(l==0){
