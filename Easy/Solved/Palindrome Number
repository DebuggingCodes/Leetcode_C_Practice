bool isPalindrome(int x) {
    if(x<0){
    return false;
    }
    int y=x;
    unsigned int inverse=0;
  int divisor=1;
  while(y>0){
  
  
  int rem= y%10;
  inverse=(inverse*10)+(rem);
  y/=10;
  divisor*=10;
  }
  
  if(x==inverse&&inverse<=INT_MAX){
      return true;
  }
  else {return false;}
}
